{
  "stryker-config": {
    "project-info": {
      "name": "github.com/lyndychivs/Wizdle",
      "module": "Wizdle",
      "version": "master"
    },
    "concurrency": 2,
    "mutation-level": "Complete",
    "language-version": "latest",
    "additional-timeout": 5000,
    "mutate": [
      "**/*",
      "!**/Program.cs"
    ],
    "solution": "Wizdle.sln",
    "configuration": "GitHub",
    "target-framework": "net8.0",
    "project": "/Wizdle/Wizdle.csproj",
    "coverage-analysis": "perTest",
    "disable-bail": false,
    "disable-mix-mutants": false,
    "thresholds": {
      "high": 80,
      "low": 60,
      "break": 0
    },
    "verbosity": "info",
    "reporters": [
      "Progress",
      "Html",
      "dashboard"
    ],
    "since": {
      "enabled": false,
      "ignore-changes-in": [],
      "target": "master"
    },
    "baseline": {
      "enabled": false,
      "provider": "disk",
      "azure-fileshare-url": "",
      "fallback-version": "master"
    },
    "dashboard-url": "https://dashboard.stryker-mutator.io",
    "test-projects": ["/Wizdle.Tests/Wizdle.Tests.csproj"],
    "test-case-filter": "",
    "ignore-mutations": [],
    "ignore-methods": [],
    "report-file-name": "mutation-report.html",
    "break-on-initial-test-failure": false
  }
}